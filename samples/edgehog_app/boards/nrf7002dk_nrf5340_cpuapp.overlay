/delete-node/ &storage_partition;

&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		storage_partition: partition@f8000 {
			label = "storage";
			reg = <0x000f8000 0x00002000>;
		};

		astarte_partition: partition@fA000 {
			label = "astarte";
			reg = <0x000fA000 0x00006000>;
		};
	};
};
